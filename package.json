    "@uniformdev/richtext": "20.37.1-alpha.11",
    "@uniformdev/tms-sdk": "20.37.1-alpha.11",
    "@vercel/og": "^0.6.2",
    "classnames": "^2.5.1",
    "daisyui": "^3.9.4",
    "framer-motion": "10.16.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.541.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-multi-carousel": "^2.8.5",
    "react-player": "^2.16.0",
